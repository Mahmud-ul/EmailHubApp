
@{
    ViewData["Title"] = "LoadFile";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@if (ViewBag.ErrorMessage != null)
{
    <div class="alert alert-danger"><strong>Error:&nbsp;</strong>@ViewBag.ErrorMessage</div>
}
<form asp-controller="File" enctype="multipart/form-data" asp-action="LoadFile" method="post">
    <h2>Upload File</h2><hr />  
    <div >
        <article>Select File</article>
        <span class="mx-auto py-1 border border-secondary border-2"><input type="file" name="File" /></span><br /><br />
        <input type="submit" value=" Read Data " />
    </div>
    
</form>


